{
  "name": "abterphp/admin",
  "description": "Admin module for AbterPHP",
  "authors": [
    {
      "name": "Peter Aba",
      "email": "peter@peteraba.com",
      "role": "Engineer"
    }
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AbterPhp\\Admin\\": [
        "src",
        "tests"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "abterphp/framework": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7",
    "phpmd/phpmd": "^2.13",
    "phan/phan": "^5.4",
    "php-coveralls/php-coveralls": "^2.5"
  },
  "config": {
    "prefer-install": "dist"
  }
}
